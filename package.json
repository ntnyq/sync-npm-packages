{
  "name": "sync-npm-packages",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@10.4.1",
  "description": "Sync released npm packages to a mirror site.",
  "keywords": [
    "npm",
    "npmmirror",
    "package",
    "registry",
    "release",
    "sync"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/sync-npm-packages#readme",
  "repository": "ntnyq/sync-npm-packages",
  "bugs": {
    "url": "https://github.com/ntnyq/sync-npm-packages/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "./bin.mjs",
  "files": [
    "bin.mjs",
    "dist",
    "schemas"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish release:sync",
    "release:check": "run-s lint typecheck",
    "release:publish": "bumpp && pnpm publish",
    "release:sync": "npx sync-npm-packages --target npmmirror",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "tinyglobby": "^0.2.12",
    "tinyrainbow": "^2.0.0",
    "unconfig": "^7.0.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.0-beta.8",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.13.5",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "esbuild": "^0.25.0"
    }
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,yml,yaml,json}": "eslint --fix"
  }
}
